{
  "Node": {
    "WebApi": {
      "Listening": "http://192.168.1.156:7000",
      "Advertise": "http://192.168.1.156:7000"
    },
    "Gossip": {
      "Listening": "0.0.0.0:7946",
      "SeedNodes": [],
      "SyncOnlyWithSeedNodes": false,
      "Name": "cypher"
    },
    "Data": {
      "rocksdb": "storedb",
      "KeysProtectionPath": "keys"
    }
  },
  "Log": {
    "FirstChanceException": false,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "Enrich": "FromLogContext",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss} [{Level:u3}] [{SourceContext}] [{MemberName}:{LineNumber}] {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact",
          "path": "cypnode.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7
        }
      }
    ]
  },
  "Staking": {
    "Enabled": false,
    "TransactionsPerBlock": 100,
    "WalletSettings": {
      "Identifier": "",
      "Passphrase": "",
      "Address": "",
      "Url": "http://localhost:8001",
      "SendPaymentEndpoint": "/api/wallet/transaction"
    }
  },
  "Network": {
    "Environment": "testnet",
    "TransactionRateConfig": {
      "LeakRate": 386,
      "LeakRateNumberOfSeconds": 5,
      "MaxFill": 1024
    }
  }
}
